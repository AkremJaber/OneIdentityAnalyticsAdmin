@model OneIdentityAnalytics.Services.PowerBiTenantDetails
    <h2>
        <span><a class="btn btn-sm h2-backarrow" asp-action="Tenants"><i class="fa fa-backward"></i></a></span>
        <span>Tenant Details</span>
    </h2>

    <form class="form-horizontal" role="form">
        <fieldset>
            <div class="form-group row">
                <label for="Name" class="col-sm-3 col-form-label">Tenant Name</label>
                <div class="col-sm-9">
                    <input class="form-control " id="Name" name="Name" readonly value="@Model.CCC_Name">
                </div>
            </div>
            @*<div class="form-group row">
                <label for="WorkspaceId" class="col-sm-3 col-form-label">Workspace ID</label>
                <div class="col-sm-9">
                    <input class="form-control" id="WorkspaceId" name="WorkspaceId" readonly="readonly" value="@Model.WorkspaceId">
                </div>
            </div>
            <div class="form-group row">
                <label for="WorkspaceUrl" class="col-sm-3 col-form-label">Workspace URL</label>
                <div class="col-sm-9">
                    <input class="form-control" id="WorkspaceUrl" name="WorkspaceUrl" readonly="readonly" value="@Model.WorkspaceUrl">
                </div>
            </div>
            <div class="form-group row">
                <label for="DatabaseServer" class="col-sm-3 col-form-label">Database Server</label>
                <div class="col-sm-9">
                    <input class="form-control" id="DatabaseServer" name="DatabaseServer" readonly="readonly" value="@Model.DatabaseServer">
                </div>
            </div>
            <div class="form-group row">
                <label for="DatabaseName" class="col-sm-3 col-form-label">Database Name</label>
                <div class="col-sm-9">
                    <input class="form-control" id="DatabaseName" name="DatabaseName" readonly="readonly" value="@Model.DatabaseName">
                </div>
            </div>
            <div class="form-group row">
                <label for="ClientSecret" class="col-sm-3 col-form-label">Database User Name</label>
                <div class="col-sm-9">
                    <input class="form-control" id="DatabaseUserName" name="DatabaseUserName" readonly="readonly" value="@Model.DatabaseUserName">
                </div>
            </div>
            <div class="form-group row">
                <label for="DatabaseUserPassword" class="col-sm-3 col-form-label">Database User Password</label>
                <div class="col-sm-9">
                    <input class="form-control" id="DatabaseUserPassword" name="DatabaseUserPassword" readonly="readonly" value="@Model.DatabaseUserPassword" type="password">
                </div>
            </div>*@
        </fieldset>
    </form>


    <div class="container-tabs">

        @*<div class="tab-members">
            <h4>Users</h4>
            <table class="table table-sm table-bordered table-striped">
                <thead class="thead-dark">
                    <tr>
                        <th style="text-align:center">First Name</th>
                        <th style="text-align:center">Last Name</th>
                        
                    </tr>
                </thead>
                @foreach (var person in  Model.Person)
                {
    <tr>
        <td style="text-align:center">@person.FirstName</td>
        <td style="text-align:center">@person.LastName</td>
        
    </tr>
}
            </table>
        </div>*@

        <div class="tab-members">
            <h4>Members</h4>
            <table class="table table-sm table-bordered table-striped">
                <thead class="thead-dark">
                    <tr>
                        <th style="text-align:center">Member</th>
                        <th style="text-align:center">Permissions</th>
                        <th style="text-align:center">Member Type</th>
                    </tr>
                </thead>
                @foreach (var member in Model.Members)
                {
    <tr>
        <td style="text-align:center">@member.DisplayName</td>
        <td style="text-align:center">@member.GroupUserAccessRight</td>
        <td style="text-align:center">@member.PrincipalType.Value</td>
    </tr>
}
            </table>
        </div>

        <div class="tab-datasets">
            <h4>Datasets</h4>
            <table class="table table-sm table-bordered table-striped">
                <thead class="thead-dark">
                    <tr>
                        <th style="text-align:center">Name</th>
                        <th style="text-align:center">Is Refreshable</th>
                        <th style="text-align:center">ID</th>
                    </tr>
                </thead>
                @foreach (var dataset in Model.Datasets)
                {
    <tr>
        <td style="text-align:center">@dataset.Name</td>
        <td style="text-align:center">@dataset.IsRefreshable</td>
        <td style="text-align:center">@dataset.Id</td>
    </tr>
}
            </table>
        </div>

        <div class="tab-reports">
            <h4>Reports</h4>
            <table class="table table-sm table-bordered table-striped">
                <thead class="thead-dark">
                    <tr>
                        <th style="text-align:center">Name</th>
                        <th style="text-align:center">Report Type</th>
                        <th style="text-align:center">ID</th>
                        <th style="text-align:center;">Embed</th>

                    </tr>
                </thead>

                @foreach (var report in Model.Reports)
                {
    <tr>

        <td style="text-align:center">@report.Name</td>
        <td style="text-align:center">@report.ReportType</td>
        <td style="text-align:center">@report.Id</td>
        <td style="text-align:center"><a href="~/Home/Embed/?ReportId=@report.Id&Tenant=@Model.CCC_Name" class="nav nav-link" target="_blank"><i class="fa fa-bar-chart" /></a></td>
    </tr>
}
            </table>

            <div class="tab-dash">
                <h4>Dashboards</h4>
                <table class="table table-sm table-bordered table-striped">
                    <thead class="thead-dark">
                        <tr>
                            <th style="text-align:center">Name</th>
                            <th style="text-align:center">ID</th>
                            <th style="text-align:center;">Embed</th>
                        </tr>
                    </thead>
                    @foreach (var dashbord in Model.Dashboard)
                    {
        <tr>
            <td style="text-align:center">@dashbord.DisplayName</td>
            <td style="text-align:center">@dashbord.Id</td>
            <td style="text-align:center"><a href="~/Home/EmbedDash/?DashboardId=@dashbord.Id&?Dashboard=@Model.Dashboard&Tenant=@Model.CCC_Name" class="nav nav-link" target="_blank"><i class="fa fa-bar-chart"></a></td>

        </tr>}
                </table>

            </div>

        </div>
    </div>